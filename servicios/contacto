    }

    .container {
        gap: 15px; /* Reduce el espacio entre los �conos en pantallas peque�as */
    }

    .linea {
        height: 0.3px; /* Reduce el grosor de la l�nea en pantallas peque�as */
    }

    h1 {
        font-size: 24px; /* Ajusta el tama�o de los t�tulos */
    }

    p {
        font-size: 14px; /* Ajusta el tama�o del texto */
    }
}

@media (max-width: 480px) {
    .modal-content1 {
        width: 90%;
        padding: 10px; /* Reduce a�n m�s el padding en pantallas muy peque�as */
        ps

    }

    .Icon {
        width: 45px;
        height: 45px; /* Asegura que los �conos no sean demasiado grandes */
    }

    h1 {
        font-size: 22px; /* Ajusta el tama�o de los t�tulos */
    }

    p {
        font-size: 12px; /* Ajusta el tama�o del texto */
    }
}

</style>
                         


