<script type="text/javascript">

	$.getScript("https://barranquillas.unisimon.edu.co/utilidades/assets/js/habeasdata.js",function () {
		$('[name=btnEnviar]').on('click', function(){
		  if ($("#frm-contacto").valid()){
			showHabeasData({
				documento:$('[name=doc]').val(), 
				codigo:0, 
				usuario:'', 
				modulo:10,  
				nombre:$('[name=nom]').val()+' '+$('[name=ape]').val(), 
				tipo_documento:$('[name=tipdoc]').val()
			}, function(){
				$('#mensaje').val($('#mensaje').val().replace(new RegExp('\\n','g'),' ').replace(new RegExp('\\r','g'),' '))
				$.ajax({
				   url:$("#varurl").attr("data-email"), //Url a donde la enviaremos
				   type:'POST', //Metodo que usaremos
				   data:$("#frm-contacto").serialize(), //Le pasamos el objeto que creamos con los archivos
				   beforeSend:function(){
					  // $("#btn").val("ENVIANDO...");
				   },
				   success: function(data){
					 if(data=="1"){
					   // location.href="";
					  document.getElementById("frm-contacto").reset();
					   alert("Gracias por escribirnos. Pronto nos pondremos en contacto con usted.")
					 } else {
						alert(data);
					 }
				   }
				});
			});
		  }
		});
		$('[name=dependencia]').on('change', function(){
			var seleccionado=$('[name=dependencia]').find('[value='+$('[name=dependencia]').val()+']');
			var ayuda="";
			if(seleccionado!=null){
				ayuda=seleccionado.data("ayuda")
			}
			$('[name=ayuda]').html(ayuda);
			var bloqueo="";
			if(seleccionado!=null){
				if(seleccionado.data("bloqueo")!=""){
					bloqueo=$.parseJSON($.base64.atob(seleccionado.data("bloqueo")));
				}
			}
			if(bloqueo!=""){
				$('[name=ayuda]').html(bloqueo);
				$('[name=nom]').css('display', 'none');
				$('[name=ape]').css('display', 'none');
				$('[name=ciu]').css('display', 'none');
				$('[name=tel]').css('display', 'none');
				$('[name=ema]').css('display', 'none');
				$('[name=cel]').css('display', 'none');
				$('[name=btnEnviar]').css('display', 'none');
				$('[name=mensaje]').css('display', 'none');
			}else{
				$('[name=nom]').css('display', '');
				$('[name=ape]').css('display', '');
				$('[name=ciu]').css('display', '');
				$('[name=tel]').css('display', '');
				$('[name=ema]').css('display', '');
				$('[name=cel]').css('display', '');
				$('[name=btnEnviar]').css('display', '');
				$('[name=mensaje]').css('display', '');
			}
		});
	});
	</script><style>

