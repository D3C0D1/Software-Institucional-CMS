<script>
    var modalPortalAula=$("[name=modalPortalAula]");
    var refbarranquillan=modalPortalAula.find("[name=refbarranquillan]");
    var refbarranquillav=modalPortalAula.find("[name=refbarranquillav]");
    refbarranquillan.on('click', function(){
        var linkBan=modalPortalAula.find("[name=linkBan]").val();
        var tipoBan=modalPortalAula.find("[name=tipoBan]").val();
        window.open(linkBan, tipoBan);
        modalPortalAula.modal('hide');
    });
    refbarranquillav.on('click', function(){
        var linkBav=modalPortalAula.find("[name=linkBav]").val();
        var tipoBav=modalPortalAula.find("[name=tipoBav]").val();
        window.open(linkBav, tipoBav);
        modalPortalAula.modal('hide');
    });
</script>

