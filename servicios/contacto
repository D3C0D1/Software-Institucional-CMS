<script>
    const reproductor = new Audio("https://sonic.paulatina.co/8174/stream");

    function estado(iconoId) {
        const icono = document.getElementById(iconoId);
        
        if (icono.classList.contains('fa-play')) {
            icono.classList.remove('fa-play');
            icono.classList.add('fa-pause');
            reproductor.play();
            icono.style.transform = "translate(-270%, 32%)";      
        } else if (icono.classList.contains('fa-pause')) {
            icono.classList.remove('fa-pause');
            icono.classList.add('fa-play');
            reproductor.pause(); 
            icono.style.transform = "translate(-190%, 32%)";      

 
        }
    }  

</script>




